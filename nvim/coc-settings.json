{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "go"
            ]
        },
        "clojure-lsp": {
          "command": "bash",
          "args": ["-c", "cd /Users/case/dev/lsp/ && lein run"],
          "filetypes": ["clojure"],
          "rootPatterns": ["project.clj"],
          "additionalSchemes": ["jar", "zipfile"],
          "trace.server": "verbose",
          "initializationOptions": {
          }
        }
    },
    "suggest.snippetIndicator": "üç¶",
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "gosnippets/UltiSnips"
    ]
}

